{
  "version": 2,
  "builds": [
    {
      "src": "build.js",
      "use": "@vercel/node"
    },
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/wsgi.py"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/wsgi.py"
    },
    {
      "src": "/library/(.*)",
      "dest": "/wsgi.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "outputDirectory": "public"
}
